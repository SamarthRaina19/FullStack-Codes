{
  "name": "music-player-frontend",
  "version": "1.0.0",
  "description": "A modern music player web application built with vanilla HTML, CSS, and JavaScript",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "dev:legacy": "live-server --port=3001 --open=/index.html --watch=css,js,components,pages --ignore=node_modules",
    "dev:node": "node server.js",
    "start": "npm run dev",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "echo 'Linting not configured - add ESLint if needed'"
  },
  "keywords": [
    "music-player",
    "spa",
    "vanilla-js",
    "frontend",
    "responsive",
    "dark-theme"
  ],
  "author": "Music Player Team",
  "license": "MIT",
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.2",
    "live-server": "^1.2.2",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "bootstrap": "^5.3.8",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "optionalDependencies": {
    "express": "^4.18.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/music-player-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/music-player-frontend/issues"
  },
  "homepage": "https://github.com/your-username/music-player-frontend#readme"
}
